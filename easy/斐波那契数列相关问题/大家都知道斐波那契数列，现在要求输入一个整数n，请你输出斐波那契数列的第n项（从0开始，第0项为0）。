class Solution {
public:
    int Fibonacci(unsigned int n) {
        if(n<=0)
            return 0;
        if(n==1)
            return 1;
        /*vector<int> f;
        f.push_back(0);
        f.push_back(1);
        for(int i=2;i<=n;i++)
            f.push_back(f[i-1]+f[i-2]);
        return f[n];*///原始解法
        //递归解法
        return Fibonacci(n-2)+Fibonacci(n-1);


    }
};
