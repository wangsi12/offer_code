class Solution {
public:
    int rectCover(int n) {
        if (n==1){
            return 1;
        }
        if (n==2){
            return 2;
        }
       vector<int>f;
        f.push_back(1);
        f.push_back(2);
        for (int i=2;i<n;i++)
        {
            f.push_back(f[i-1]+f[i-2]);
        }
        return f[n-1];
    }
};
